        - conFormButtons:
            Control: GroupContainer
            Variant: horizontalAutoLayoutContainer
            Properties:
              DropShadow: =DropShadow.None
              FillPortions: =0
              Height: =80
              LayoutAlignItems: =LayoutAlignItems.Center
              LayoutGap: =20
              LayoutJustifyContent: =LayoutJustifyContent.End
              LayoutMode: =LayoutMode.Auto
              PaddingLeft: =20
              PaddingRight: =20
            Children:
            - conButtons_frm:
                Control: GroupContainer
                Variant: horizontalAutoLayoutContainer
                Properties:
                  DropShadow: =DropShadow.None
                  LayoutAlignItems: =LayoutAlignItems.Center
                  LayoutGap: =20
                  LayoutJustifyContent: =LayoutJustifyContent.End
                  LayoutMinHeight: =10
                  LayoutMinWidth: =10
                  LayoutMode: =LayoutMode.Auto
                Children:
                - btnPrevious:
                    Control: Classic/Button
                    Properties:
                      OnSelect: =If(galFormSteps.Selected.order >1, UpdateContext({locStep:LookUp(colProgressSteps, order = galFormSteps.Selected.order-1)}))
                      Text: ="Back"
                      BorderColor: =RGBA(59,130,246,1)
                      Color: =RGBA(59,130,246,1)
                      Fill: =RGBA(255, 255, 255, 1)
                      Font: =Font.Lato
                      FontWeight: =FontWeight.Normal
                      Height: =50
                      HoverFill: =ColorFade(Self.Fill, -20%)
                      Size: =13
                      Visible: =galFormSteps.Selected.order >1 //And frmRequest.Mode <> FormMode.View And isNewForm
                      Width: =200
                - btnNext:
                    Control: Classic/Button
                    Properties:
                      OnSelect: =UpdateContext({locStep:LookUp(colProgressSteps, order = galFormSteps.Selected.order+1)})
                      Text: ="Continue to Resource Group"
                      BorderStyle: =BorderStyle.None
                      DisplayMode: |-
                        =If(
                            Switch(
                                galFormSteps.Selected.order,
                                1,
                                !IsBlank(cmbSubscriptions.Selected.displayName) And!If(
                                    TabList1.Selected.Value = "Use Existing",
                                    IsBlank(locSG.name),
                                    IsBlank(Input_RG.Value) And IsBlank(cmbLocation.Selected.displayName)
                                ) And CountIf(colRGmandatory, IsBlank(tagValue)) = 0,
                                2,
                                !IsBlank(cmbResourceType.Selected.Value) And Switch(
                                    cmbResourceType.Selected.Value,
                                    'ResourceType (Requests)'.'VM Windows',
                                    //!IsBlank(cmbVmSize.Selected.Value) And!IsBlank(InputDescription.Value) And 
                                    CountIf(colVMtags, IsBlank(tagValue)) = 0,
                                    'ResourceType (Requests)'.'Storage Account',
                                    true,
                                    'ResourceType (Requests)'.'Key Vault',
                                    !IsBlank(inputAllowedIpAddresses.Value) And!IsBlank(inputAllowedSubnetIDs.Value) And!IsBlank(inputKeyVaultSecretUsers.Value)
                                )
                            ),
                            DisplayMode.Edit,
                            DisplayMode.Disabled
                        )
                        /*If( 
                           Or( !IsBlank( cmbSubscriptions.Selected.displayName) And  !If(TabList1.Selected.Value = "Use Existing",IsBlank( locSG.name),IsBlank( Input_RG.Value) And IsBlank(cmbLocation.Selected.displayName)) ,
                            !IsBlankOrError( cmbVmSize.Selected.Value) And !IsBlankOrError(InputDescription.Value))
                         , DisplayMode.Edit,DisplayMode.Disabled)
                         */
                      Fill: =RGBA(59,130,246,1)
                      Font: =Font.Lato
                      FontWeight: =FontWeight.Normal
                      Height: =50
                      HoverFill: =ColorFade(Self.Fill, -20%)
                      Size: =13
                      Visible: =galFormSteps.Selected.order <> galFormSteps.AllItemsCount
                      Width: =270
                - btnDraft:
                    Control: Classic/Button
                    Properties:
                      OnSelect: |-
                        =SubmitForm(frmRequest); 
                        Patch('Self Service Logs',Defaults('Self Service Logs'),{'Action Name':"New "&DataCardValue7.Selected.Value,'Action Type': 'Action Type'.Create,'Activity Status': 'Activity Status (Self Service Logs)'.Success}); Navigate(srcUserMain)
                      Text: ="Draft"
                      BorderStyle: =BorderStyle.None
                      Color: =RGBA(0, 0, 0, 1)
                      Fill: =RGBA(214, 221, 224, 1)
                      Font: =Font.Lato
                      FontWeight: =FontWeight.Normal
                      Height: =50
                      HoverFill: =ColorFade(Self.Fill, -20%)
                      Size: =13
                      Visible: =!btnNext.Visible And locSelectedItem.'Request Status' <> 'Request Status (Requests)'.Submitted And frmRequest.Mode <> FormMode.View And isNewForm
                      Width: =200
                - btnSubmit:
                    Control: Classic/Button
                    Properties:
                      OnSelect: |-
                        =UpdateContext({locStatus: 'Request Status (Requests)'.Submitted});
                        SubmitForm(frmRequest); 
                        Patch('Self Service Logs',Defaults('Self Service Logs'),{'Action Name':"",'Action Type': 'Action Type'.Create,'Activity Status': 'Activity Status (Self Service Logs)'.Success})
                      Text: ="Deploy"
                      BorderStyle: =BorderStyle.None
                      Fill: =RGBA(59,130,246,1)
                      Font: =Font.Lato
                      FontWeight: =FontWeight.Normal
                      Height: =50
                      HoverFill: =ColorFade(Self.Fill, -20%)
                      Size: =13
                      Visible: =!btnNext.Visible And locSelectedItem.'Request Status' <> 'Request Status (Requests)'.Submitted And frmRequest.Mode <> FormMode.View And isNewForm
                      Width: =200
